package edu.projecteuler.collatzsequence;

public class CollatzSequence {

	private static void getChainLength(int start) {
		int current = start;
		while (current > 0) {
			if(current % 2 == 0){
				current = current / 2;
			} else {
				current = 3 * current + 1;
			}
		}
	}

	public static void main(String[] args) {

	}
}
