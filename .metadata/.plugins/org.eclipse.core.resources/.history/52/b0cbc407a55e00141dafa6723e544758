package edu.projecteuler.circularprimes;

public class CircularPrimes {

	private static boolean isPrime(int x) {
		final int end = (int) Math.sqrt(x);
		for (int i = 2; i < end; ++i) {
			if (x % i == 0) {
				return false;
			}
		}
		return true;
	}
	
	private static int rotateNumber(int number){
		final String numString = Integer.toString(number);
		final char[] chars = numString.toCharArray();
		for(int i = 0; i < chars.length / 2; ++i){
			char swap = chars[i];
			chars[]
		}
	}

	public static void main(String[] args) {
		
	}
}
